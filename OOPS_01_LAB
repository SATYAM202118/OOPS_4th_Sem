#include<iostream>
using namespace std;
void matrix(){

    int row,col;
    cout<<"Enter the size of matrix: "<<endl;
    cin>>row >> col;
    int a[row][col];
    
    cout<<"Enter the values: "<<endl;
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cin>>a[i][j];
        }
    }

    cout << "The matrix is " << endl;
    for(int i=0;i<row;i++){
        for(int j=0;j<col;j++){
            cout<<a[i][j]<<" ";
        }
        cout<<endl;
    }
}
int main(){
    matrix();
return 0;
}
